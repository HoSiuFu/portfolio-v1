{
    "editor.codeActionsOnSave": {
        "source.fixAll": "always"
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "stylelint.snippet": [
        "css",
        "less",
        "postcss",
        "scss"
    ],
    "stylelint.validate": [
        "css",
        "less",
        "postcss",
        "scss"
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    }
}